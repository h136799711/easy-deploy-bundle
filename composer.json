{
    "name": "dbh/easy-deploy-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 6.1, modify from EasyCorp/easy-deploy-bundle",
    "keywords": ["deploy", "deployment", "deployer"],
    "homepage": "https://github.com/h136799711/easy-deploy-bundle",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=8.1",
        "symfony/console": "6.1.*",
        "symfony/dependency-injection": "6.1.*",
        "symfony/expression-language": "6.1.*",
        "symfony/filesystem": "6.1.*",
        "symfony/http-foundation": "6.1.*",
        "symfony/http-kernel": "6.1.*",
        "symfony/polyfill-mbstring": "1.25.*",
        "symfony/process": "6.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDeployBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDeployBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
